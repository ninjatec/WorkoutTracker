@page
@model WorkoutTrackerWeb.Pages.Settypes.IndexModel

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-page="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Settype[0].Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Settype[0].Description)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model.Settype) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Description)
            </td>
            <td>
                <a asp-page="./Edit" asp-route-id="@item.SettypeId">Edit</a> |
                <a asp-page="./Details" asp-route-id="@item.SettypeId">Details</a> |
                <a asp-page="./Delete" asp-route-id="@item.SettypeId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
