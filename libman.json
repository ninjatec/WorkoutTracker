{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "@microsoft/signalr@8.0.0",
      "destination": "wwwroot/lib/microsoft/signalr",
      "files": [
        "dist/browser/signalr.js",
        "dist/browser/signalr.min.js"
      ],
      "provider": "unpkg"
    },
    {
      "library": "Chart.js@4.3.0",
      "destination": "wwwroot/lib/chartjs"
    },
    {
      "library": "moment.js@2.29.4",
      "destination": "wwwroot/lib/moment"
    },
    {
      "provider": "unpkg",
      "library": "daterangepicker@3.1.0",
      "destination": "wwwroot/lib/daterangepicker",
      "files": [
        "daterangepicker.js",
        "daterangepicker.css"
      ]
    },
    {
      "library": "toastr.js@2.1.4",
      "destination": "wwwroot/lib/toastr",
      "files": [
        "toastr.min.js",
        "toastr.min.css"
      ]
    },
    {
      "library": "datatables@1.10.21",
      "destination": "wwwroot/lib/datatables",
      "files": [
        "js/jquery.dataTables.min.js",
        "css/jquery.dataTables.min.css"
      ]
    },
    {
      "provider": "unpkg",
      "library": "chartjs-adapter-date-fns@3.0.0",
      "destination": "wwwroot/lib/chartjs-adapter-date-fns"
    },
    {
      "library": "tinymce@6.7.0",
      "destination": "wwwroot/lib/tinymce",
      "files": [
        "tinymce.min.js",
        "themes/silver/theme.min.js",
        "models/dom/model.min.js",
        "plugins/image/plugin.min.js",
        "plugins/link/plugin.min.js",
        "plugins/lists/plugin.min.js",
        "plugins/table/plugin.min.js",
        "plugins/code/plugin.min.js",
        "plugins/codesample/plugin.min.js",
        "plugins/autolink/plugin.min.js",
        "plugins/autoresize/plugin.min.js",
        "plugins/preview/plugin.min.js",
        "plugins/media/plugin.min.js",
        "plugins/pagebreak/plugin.min.js",
        "plugins/wordcount/plugin.min.js",
        "skins/ui/oxide/skin.min.css",
        "skins/ui/oxide/content.min.css",
        "skins/content/default/content.min.css",
        "icons/default/icons.min.js"
      ]
    }
  ]
}
